<%= simple_form_for [:settings, position] do |f| %>
  <%= f.input :name %>

  <% if 1 == 2 %>
    <%= f.association :managees,
      as: :check_boxes,
      collection: current_company.positions.reject(:blank?)
    %>
  <% end %>

  <%= f.submit %> <%= link_to "Back", settings_positions_path %>
<% end %>
